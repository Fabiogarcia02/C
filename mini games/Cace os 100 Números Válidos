#include <stdio.h>

int terminaComSeis(int numero) {
    return numero % 10 == 6;
}

int main() {
    int vetor[100];
    int i = 0, numero = 1;

    printf("ğŸ¯ Bem-vindo ao jogo dos 100 NÃºmeros VÃ¡lidos!\n");
    printf("â¡ï¸ Preenchendo com nÃºmeros que NÃƒO sÃ£o mÃºltiplos de 6 e NÃƒO terminam com 6...\n");

    while(i < 100) {
        if(numero % 6 != 0 && !terminaComSeis(numero)) {
            vetor[i] = numero;
            i++;
        }
        numero++;
    }

    printf("\nâœ… Os 10 primeiros nÃºmeros vÃ¡lidos encontrados:\n");
    for(int j = 0; j < 10; j++) {
        printf("%d ", vetor[j]);
    }

    printf("\nğŸ Fim do jogo!\n");
    return 0;
}
